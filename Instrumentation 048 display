rule "Primo VE - Lds05"
	when
		MARC is "048"."a"
	then
                set TEMP"1" to MARC."048"."a"
                remove substring using regex (TEMP"1","(?<numbers>[0-9]+)")

                replace string by string (TEMP"1","\\bba\\b","Horn")
                replace string by string (TEMP"1","\\bbb\\b","Trumpet")
                replace string by string (TEMP"1","\\bbc\\b","Cornet")
                replace string by string (TEMP"1","\\bbd\\b","Trombone")
