                replace string by string (TEMP"1","\\bca\\b","Choruses, Mixed")
                replace string by string (TEMP"1","\\bcb\\b","Choruses, Women's")
                replace string by string (TEMP"1","\\bcc\\b","Choruses, Men's")
                replace string by string (TEMP"1","\\bcd\\b","Choruses, Children's")
                replace string by string (TEMP"1","\\bcn\\b","Choruses, Unspecified")
                replace string by string (TEMP"1","\\bcu\\b","Choruses, Unknown")
                replace string by string (TEMP"1","\\bcy\\b","Choruses, Ethnic")
                replace string by string (TEMP"1","\\bea\\b","Synthesizer")
