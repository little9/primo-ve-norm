                replace string by string (TEMP"1","\\bed\\b","Ondes Martinot")
                replace string by string (TEMP"1","\\ben\\b","Electronic, Unspecified")
                replace string by string (TEMP"1","\\beu\\b","Electronic, Unknown")
                replace string by string (TEMP"1","\\bez\\b","Electronic, Other")
                replace string by string (TEMP"1","\\bka\\b","Piano")
                replace string by string (TEMP"1","\\bkb\\b","Organ")
                replace string by string (TEMP"1","\\bkc\\b","Harpsichord")
                replace string by string (TEMP"1","\\bkd\\b","Clavichord")
